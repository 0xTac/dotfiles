    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false,
        "<C-c>": false,
        "<C-v>": false
    },
    "python.pythonPath": "/usr/bin/python",
    "material-icon-theme.showWelcomeMessage": false,
